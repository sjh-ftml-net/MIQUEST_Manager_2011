FROM JOURNALS (LATEST FOR PATIENT)
WHERE ACTIVE IN ("R")
AND CODE IN ("X00S6%","X00SL","X00SM%","X00SJ%","XSGon","E11z.","E11z0","E11zz","XE1Ze","XE1ZZ","XaX54","XaX53")
