FOR LTC016G
REPORT
PRINT NHS_NUMBER,REFERENCE,HCP,HCP_TYPE,CODE,RUBRIC,DATE
FROM JOURNALS (LATEST FOR PATIENT)
WHERE ACTIVE IN ("R")
AND CODE IN ("XE13k%")
    AND CODE NOT_IN ("D1045%","X20Ck","X20Cl","X20Cj","X20Cr","X20Cu","X20Cv")
    AND DATE IN ("<ltc_start_date>"-"<ltc_end_date>")
