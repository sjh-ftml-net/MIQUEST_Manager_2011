FOR QOF005G
REPORT
PRINT NHS_NUMBER,REFERENCE,HCP,HCP_TYPE,CODE,RUBRIC,DATE
FROM JOURNALS (LATEST FOR PATIENT)
WHERE CODE IN ("1Z12"-"1Z16","1Z1B"-"1Z1L","1Z10","1Z11","1Z17"-"1Z1A")
        AND DATE IN ("<ltc_start_date>"-"<ltc_end_date>")
        AND CHOSEN.CODE IN ("1Z10","1Z11","1Z17"-"1Z1A")
   PRINT NHS_NUMBER,REFERENCE,HCP,HCP_TYPE,CODE,RUBRIC,DATE
    FROM JOURNALS (ALL FOR PATIENT)
    WHERE CODE IN ("K02%","K03%","K05%","K06","K08%","K0A","K0A1%","K0A3%","K0A5%","K0B","K0D","K0z","Kyu","D215%")
    AND DATE IN ("<ltc_start_date>"-"<ltc_end_date>")
